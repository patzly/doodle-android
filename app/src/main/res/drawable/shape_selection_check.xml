<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <solid android:color="#00000000" />
            <size
                android:width="48dp"
                android:height="48dp" />
        </shape>
    </item>

    <item
        android:id="@+id/icon_selection_check"
